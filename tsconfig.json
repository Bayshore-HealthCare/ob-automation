{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "nodenext",
        "strict": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
        "pages/*": ["./pages/*"],
        "utils/*": ["./utils/*"]
        },
        "types": [
            "node",
            "@playwright/test",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/mocha-framework",
            "@wdio/appium-service"
        ],
    },
    "include": [
        "test",
        "tests",
        "testsmob",
        "pages",
        "utils",
        "wdio.conf.ts"
    ]
}